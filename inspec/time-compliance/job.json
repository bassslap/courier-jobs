{
  "item": {
    "id": "fb69a67c-10a3-4f5e-a43e-816dcff08f3c",   ### EXAMPLE JOB ID
    "name": "INSPEC:Time compliance_v0.1.2 (InSpec from GitHub)",
    "description": "RUN THE INSPEC TIME",
    "scheduleRule": "immediate",
    "actions": {
      "accessMode": "agent",
      "steps": [
        {
          "name": "Execute time-compliance profile",
          "interpreter": {
            "name": "chef-platform/inspec-interpreter",
            "skill": { 
              "minVersion": "1.0.0",
              "maxVersion": "20.0.0"
            }
          },
          "command": {
            "exec": "scan",
            "args": {
              "path": "https://github.com/bassslap/time-compliance"
            }
          },
          "retryCount": 0,
          "timeoutSeconds": 1800,
          "failureBehavior": { "action": "retryThenFail" }
        }
      ]
    },
    "target": {
      "executionType": "sequential",
      "groups": [
        {
          "batchSize": {
            "type": "percent",
            "value": 100
          },
          "distributionMethod": "batching",
          "earlyTermination": false,
          "filter": null,
          "filterId": "00000000-0000-0000-0000-000000000000",
          "listId": "bf0a60d6-08d8-4922-8ec3-1bcab1cf677a",  ### Example of a saved Node List ID for 1 to n nodes
          "nodeIdentifiers": null,
          "nodeListType": "savedList",
          "successCriteria": [
            {
              "numRuns": {
                "type": "percent",
                "value": 100
              },
              "status": "success"
            }
          ],
          "timeoutSeconds": 240
        }
      ]
    }
  }
}
